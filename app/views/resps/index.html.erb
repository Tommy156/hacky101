<p id="notice"><%= notice %></p>

<h1>Listing Resps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Heart beat</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resps.each do |resp| %>
      <tr>
        <td><%= resp.name %></td>
        <td><%= resp.heart_beat %></td>
        <td><%= resp.latitude %></td>
        <td><%= resp.longitude %></td>
        <td><%= link_to 'Show', resp %></td>
        <td><%= link_to 'Edit', edit_resp_path(resp) %></td>
        <td><%= link_to 'Destroy', resp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resp', new_resp_path %>
