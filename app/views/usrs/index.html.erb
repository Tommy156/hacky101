<p id="notice"><%= notice %></p>

<h1>Listing Usrs</h1>

<table>
  <thead>
    <tr>
      <th>Mac </th>
      <th>Name </th>
      <th>Heart beat </th>
      <th>Self latitude </th>
      <th>Self longitude </th>
      <th>Sanc latitude </th>
      <th>Sanc longitude </th>
      <th>Direction </th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usrs.each do |usr| %>
      <tr>
        <td><%= usr.mac %></td>
        <td><%= usr.name %></td>
        <td><%= usr.heart_beat %></td>
        <td><%= usr.self_latitude %></td>
        <td><%= usr.self_longitude %></td>
        <td><%= usr.sanc_latitude %></td>
        <td><%= usr.sanc_longitude %></td>
        <td><%= usr.direction %></td>
        <td><%= link_to 'Show', usr %></td>
        <td><%= link_to 'Edit', edit_usr_path(usr) %></td>
        <td><%= link_to 'Destroy', usr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usr', new_usr_path %>
